{
    "name": "sanmai/trycatch",
    "type": "library",
    "description": "Exception-handling callable wrapper",
    "keywords": [
        "php",
        "Exception"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "*",
        "satooshi/php-coveralls": "^2",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "TryCatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\TryCatch\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
